import java.util.Scanner;

public class dwdqw {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		  int[] shu = new int[11];
		    int[] xin =new int[11];
		    Scanner input = new Scanner(System.in);
		    for(int i=0;i<10;++i){
		    	shu[i]=input.nextInt();
		    }
		    
		    
		    System.out.println("Enter ten numbers:");

		    int j;
		    int flag=0;
		    xin[0]=shu[0];
		    for(int i=1;i<10;++i){
		    	int b=0;
		    	for(j=0;j<i;++j){
		    		if(xin[j]==shu[i])
		    			break;
		    		else flag++;
		    		b=j+1;
		    	}
		    	if(flag==i)
		    		xin[b]=shu[i];
		    }
		    System.out.println("The number of distinct number"+xin.length);
		    for(j=0;j<xin.length;j++){
		    	System.out.print(xin[j]);
		    }
		    
	}

}
